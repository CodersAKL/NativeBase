{
  "name": "native-base",
  "description": "Essential cross-platform UI components for React Native",
  "keywords": [
    "android",
    "ios",
    "react",
    "native",
    "react-native",
    "native-modules",
    "react-component",
    "navbar",
    "packager"
  ],
  "version": "2.13.8",
  "main": "dist/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "typings": "./index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "Fonts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekyAnts/NativeBase.git"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/NativeBase"
  },
  "scripts": {
    "test": "jest --clearCache && jest --noStackTrace --notify --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "generate": "plop",
    "compile": "rimraf dist/* && tsc -p ./",
    "transpile": "npm run compile && cp src/basic/Icon/NBIcons.json dist/src/basic/Icon",
    "prepublish": "npm run transpile",
    "postinstall": "node .scripts/addEjectScript.js",
    "build:js": "rimraf dist/* && cross-env BABEL_ENV=production babel src --out-dir dist --source-maps --extensions .ts,.tsx",
    "build:types": "dts-generator --name native-base --project ./tsconfig.json --out index.d.ts --main native-base/index --prefix native-base",
    "build:r": "rimraf dist/* && rollup -c"
  },
  "dependencies": {
    "blueimp-md5": "^2.12.0",
    "color": "^3.1.2",
    "lodash": "^4.17.15",
    "native-base": "^2.13.8",
    "native-base-shoutem-theme": "^0.3.1",
    "react": "16.12.0",
    "react-native": "0.61.5",
    "react-native-drawer": "^2.5.1",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^4.1.0",
    "react-navigation-stack": "^2.0.16",
    "react-timer-mixin": "^0.13.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.7",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@rollup/plugin-typescript": "^3.0.0",
    "@types/blueimp-md5": "^2.7.0",
    "@types/color": "^3.0.1",
    "@types/detox": "^14.5.2",
    "@types/enzyme": "^3.10.4",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/jasmine": "^3.5.2",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/prettier": "^1.19.0",
    "@types/react-native": "^0.61.9",
    "@types/react-native-drawer": "^2.5.2",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.19.2",
    "acorn-jsx": "^5.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-inline-json-import": "^0.3.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-typescript-to-proptypes": "^1.2.1",
    "cross-env": "^7.0.2",
    "detox": "^15.1.4",
    "dts-generator": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^25.1.0",
    "jsdom": "^16.2.2",
    "metro-react-native-babel-preset": "^0.59.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "print-message": "^3.0.1",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.12.0",
    "react-to-typescript-definitions": "^2.0.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "ts-jest": "^25.3.1",
    "tsc-silent": "^1.2.1",
    "tslib": "^1.11.1",
    "typescript": "^3.8.0-beta",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "react": ">=16.4.1",
    "react-native": ">=0.56.0"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/demonas.app",
        "build": "npm run build:ios",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    }
  }
}
